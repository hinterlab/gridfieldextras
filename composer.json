{
	"name" : "internetrix/silverstripe-gridfieldextras",
	"description" : "Some useful gridfield extensions and extras",
	"type" : "silverstripe-vendormodule",
	"homepage": "https://gitlab.internetrix.net/silverstripe-modules/gridfieldextras",
	"repositories" : [
		{
			"type" : "git",
			"url" : "https://github.com/wilr/silverstripe-dropzone.git",
			"name" : "unclecheese/dropzone"
		},{
			"type" : "git",
			"url" : "git@gitlab.internetrix.net:silverstripe-modules/gridfieldrelationhandler.git",
			"name" : "simonwelsh/gridfieldrelationhandler"
		}
	],
	"keywords" : [
		"silverstripe",
		"gridfield",
		"filter",
		"upload",
		"linkable",
		"sortable"
	],
	"license": "BSD-3-Clause",
	"authors" : [{
			"name" : "Guy Watson",
			"email" : "guy.watson@internetrix.com.au"
		}
	],
	"require" : {
		"silverstripe/framework" : "~4",
		"unclecheese/dropzone" : "dev-features/4.0-support",
		"undefinedoffset/sortablegridfield" : "2.0.8",
		"simonwelsh/gridfieldrelationhandler" : "dev-ss4",
		"symbiote/silverstripe-gridfieldextensions" : "^3.2"
	},
	"extra" : {
		"installer-name" : "gridfieldextras",
		"expose": [
			"client"
		]
	},
	"autoload": {
		"psr-4": {
			"Internetrix\\GridFieldExtras\\": "src/"
		}
	},
	"prefer-stable": true,
	"minimum-stability" : "dev"
}